<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>central</id>
            <mirrorOf>central</mirrorOf>
            <url>https://repo.maven.apache.org/maven2</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>gitlab</id>
            <repositories>
                <repository>
                    <id>gitlab-group-maven</id>
                    <url>https://gitlab.com/api/v4/groups/86358211/-/packages/maven</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>gitlab</activeProfile>
    </activeProfiles>

    <servers>
        <server>
            <id>gitlab-group-maven</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Private-Token</name>
                        <value>glpat-PfUKe4CLZqby9rEPAF8e</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>
</settings>